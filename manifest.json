{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"kappafeed",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"Michael Upchurch",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"kappafeed"
	},
	"data": {
